    "babel-loader": "^8.2.2",
    "compass-sass-mixins": "^0.12.7",
    "css-loader": "^5.2.7",
    "eslint": "^5.16.0",
    "eslint-config-jquery": "^1.0.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-jquery": "^1.5.1",
    "html-loader": "^0.5.5",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.50.0",
