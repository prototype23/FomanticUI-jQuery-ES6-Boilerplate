{
  "private": true,
  "name": "FomanticUI-jQuery-ES6-Boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate for advanced content and forms.",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack --config ./webpack.dev.js",
    "build:prod": "webpack --config ./webpack.prod.js",
    "watch": "webpack --watch --config ./webpack.dev.js",
    "serve": "webpack serve --config ./webpack.dev.js",
    "dev": "run-p watch serve",
    "prod": "run-s build:prod serve",
    "stats": "\"webpack --profile --config webpack.prod.js --json > bundle-stats.json\" && webpack-bundle-analyzer bundle-stats.json ./dist/js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
